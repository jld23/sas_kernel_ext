{
  "name": "jl-create-sas-file",
  "version": "0.1.0",
  "description": "A small extension to add create SAS file to tool bar",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jld23/sas_kernel_ext",
  "bugs": {
    "url": "https://github.com/jld23/sas_kernel_ext/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jared Dean",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jld23/sas_kernel_ext.git"
  },
  "scripts": { 
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo", 
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": ">=2.0.0",
    "@jupyterlab/filebrowser": ">=2.0.0",
    "@jupyterlab/launcher": ">=2.0.0",
    "@jupyterlab/ui-components": ">=2.0.0",
    "@jupyterlab/mainmenu": ">=2.0.0"

  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2", 
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": { 
    "extension": true,
    "outputDir": "jl-create-sas-file"
  }
}
