{
  "name": "jl-sas-to-nb",
  "version": "0.1.0",
  "description": "A Jupyter Lab extension to convert a sas file to a notebook using the SAS_kernel",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jld23/sas_kernel_ext",
  "bugs": {
    "url": "https://github.com/jld23/sas_kernel_ext/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jared Dean",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jld23/sas_kernel_ext.git"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "cd jl_sas_to_nb && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jl_sas_to_nb/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": ">=2.0.0",
    "@jupyterlab/apputils": ">=2.1.1",
    "@jupyterlab/coreutils": "^4.2.5",
    "@jupyterlab/filebrowser": ">=2.1.2",
    "@jupyterlab/launcher": ">=2.0.0",
    "@jupyterlab/services": "^5.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": ">=2.25.0",
    "@typescript-eslint/parser": ">=2.25.0",
    "eslint": ">=6.8.0",
    "eslint-config-prettier": ">=6.10.1",
    "mkdirp": ">=1.0.3",
    "prettier": "1.16.4",
    "rimraf": ">=2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jl_sas_to_nb"
        }
      }
    },
    "extension": true
  }
}
